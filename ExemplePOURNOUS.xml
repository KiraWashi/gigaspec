<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- start : c'est l'id de l'expression racine -->
<Root model="#1">
	<!-- chaque element a un identifiant unique (attribut id) -->
	<!-- Pour associer deux expressions on utilise l'identifiant -->
	<!-- 	exemple : le model de l'entite #30 est pointé par son attribut model avec #1  -->
	<Model id="#1" />
	<Entity model="#1" name="Balise">
		<Attribute name="nom" type="String"/>
		<Attribute name="id" type="Integer"/>
		<Attribute name="parent" type="Flotte"/>
	</Entity>
	<Entity model="#1" name="Satellite">
		<Attribute name="nom" type="String"/>
		<Attribute name="id" type="Integer"/>
		<Attribute name="parent" type="Flotte"/>
	</Entity>
	<Entity model="#1" name="Flotte"/>
</Root>