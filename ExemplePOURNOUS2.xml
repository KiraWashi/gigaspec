<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- start : c'est l'id de l'expression racine -->
<Root model="#1">
	<!-- chaque element a un identifiant unique (attribut id) -->
	<!-- Pour associer deux expressions on utilise l'identifiant -->
	<!--     exemple : le model de l'entite #30 est pointé par son attribut model avec #1  -->
	<Model id="#1" />
	<Entity model="#1" id="#20" name="Balise"/>
	<Entity id="#90" name="Flotte" model="#1"/>
	<ReferenceType id="#91" name="Flotte" model="#1"/>
	<Attribute entity="#30" name="flotte" type="#90"/>
	<Attribute name="satellites" type="#100" entity="#90"/>
	<CollectionType id="#100" name="List" baseType="#30" minSize="5" maxSize="10"/>
	<Entity model="#1" id="#30" name="Satellite"/>

	<Attribute id ="#50" name="specialities" type="#200"/>
	<CollectionType name="List" id="#200" baseType="#300"/>
	<CollectionType name="List" id="#300" baseType="#400"/>
	<PrimitiveType name="Integer" id="#400"/>
</Root>